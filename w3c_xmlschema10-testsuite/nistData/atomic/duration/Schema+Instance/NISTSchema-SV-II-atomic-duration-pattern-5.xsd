<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns="NISTSchema-SV-II-atomic-duration-pattern-5-NS"
        targetNamespace="NISTSchema-SV-II-atomic-duration-pattern-5-NS">

  <xs:annotation>
    <xs:documentation>
      The National Institute of Standards and Technology.
      Conformance test for W3C XML Schema Part 2: Datatypes.
      Created Jan 13, 2004.

      File: NISTSchema-SV-II-atomic-duration-pattern-5.xsd
      This schema specifies a datatype, NISTSchema-SV-II-atomic-duration-pattern-5,
      which is derived by restriction of built-in type duration
      by facet pattern with a value of: P17\d\dY\d7M\d3DT0\dH0\dM\d5S
      This schema is intended to be valid with
      regard to the schema for schemas.
    </xs:documentation>
  </xs:annotation>

  <xs:element name="NISTSchema-SV-II-atomic-duration-pattern-5" type="NISTSchema-SV-II-atomic-duration-pattern-5-Type"/>

  <xs:simpleType name="NISTSchema-SV-II-atomic-duration-pattern-5-Type">
    <xs:restriction base="xs:duration">
      <xs:pattern value="P17\d\dY\d7M\d3DT0\dH0\dM\d5S"/>
    </xs:restriction>
  </xs:simpleType>

</xs:schema>
