<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns="NISTSchema-SV-IV-atomic-duration-pattern-4-NS"
        targetNamespace="NISTSchema-SV-IV-atomic-duration-pattern-4-NS">

  <xs:annotation>
    <xs:documentation>
      The National Institute of Standards and Technology.
      Conformance test for W3C XML Schema Part 2: Datatypes.
      Created Jan 13, 2004.

      File: NISTSchema-SV-IV-atomic-duration-pattern-4.xsd
      This schema specifies a datatype, NISTSchema-SV-IV-atomic-duration-pattern-4,
      which is derived by restriction of built-in type duration
      by facet pattern with a value of: P19\d\dY\d8M\d3DT\d0H1\dM\d2S
      This schema is intended to be valid with
      regard to the schema for schemas.
    </xs:documentation>
  </xs:annotation>

  <xs:element name="NISTSchema-SV-IV-atomic-duration-pattern-4" type="NISTSchema-SV-IV-atomic-duration-pattern-4-Type"/>

  <xs:simpleType name="NISTSchema-SV-IV-atomic-duration-pattern-4-Type">
    <xs:restriction base="xs:duration">
      <xs:pattern value="P19\d\dY\d8M\d3DT\d0H1\dM\d2S"/>
    </xs:restriction>
  </xs:simpleType>

</xs:schema>
