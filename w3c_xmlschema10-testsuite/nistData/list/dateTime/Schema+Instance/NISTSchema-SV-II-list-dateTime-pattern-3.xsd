      <pattern value="20\d\d-\d0-2\dT\d9:5\d:\d9 \d\d34-\d4-2\dT0\d:2\d:1\d 18\d\d-\d0-\d1T1\d:0\d:\d3 \d\d10-0\d-2\dT1\d:1\d:4\d 18\d\d-\d3-0\dT1\d:\d8:\d2 17\d\d-\d4-1\dT1\d:3\d:0\d"/>
    </restriction>
  </simpleType>

</schema>
