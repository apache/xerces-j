<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:nist="NISTSchema-SV-II-list-gMonth-pattern-2-NS"
        targetNamespace="NISTSchema-SV-II-list-gMonth-pattern-2-NS">

  <annotation>
    <documentation>
      The National Institute of Standards and Technology.
      Conformance test for W3C XML Schema Part 2: Datatypes.
      Created Jan 13, 2004.

      File: NISTSchema-SV-II-list-gMonth-pattern-2.xsd
      This schema specifies a datatype, NISTSchema-SV-II-list-gMonth-pattern-2,
      which is derived by restriction of a list with item type gMonth
      by facet pattern with a value of: --\d1 --\d8 --\d8 --\d2 --0\d --0\d --\d4 --0\d
      This schema is intended to be valid with
      regard to the schema for schemas.
    </documentation>
  </annotation>

  <element name="NISTSchema-SV-II-list-gMonth-pattern-2" type="nist:NISTSchema-SV-II-list-gMonth-pattern-2-Type"/>

  <simpleType name="listOfgMonth">
    <list itemType="gMonth"/>
  </simpleType>

  <simpleType name="NISTSchema-SV-II-list-gMonth-pattern-2-Type">
    <restriction base="nist:listOfgMonth">
      <pattern value="--\d1 --\d8 --\d8 --\d2 --0\d --0\d --\d4 --0\d"/>
    </restriction>
  </simpleType>

</schema>
