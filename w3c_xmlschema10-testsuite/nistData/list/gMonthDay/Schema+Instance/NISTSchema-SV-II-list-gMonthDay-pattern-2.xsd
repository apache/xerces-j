  <element name="NISTSchema-SV-II-list-gMonthDay-pattern-2" type="nist:NISTSchema-SV-II-list-gMonthDay-pattern-2-Type"/>

  <simpleType name="listOfgMonthDay">
    <list itemType="gMonthDay"/>
  </simpleType>

  <simpleType name="NISTSchema-SV-II-list-gMonthDay-pattern-2-Type">
    <restriction base="nist:listOfgMonthDay">
      <pattern value="--0\d-\d6 --\d3-\d2 --\d9-\d7 --\d2-\d7 --0\d-\d1 --0\d-1\d --0\d-\d5 --0\d-0\d --\d9-\d9"/>
