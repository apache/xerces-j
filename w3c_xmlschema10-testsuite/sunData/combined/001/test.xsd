									<xs:element name="foo" type="empty"/>
								</xs:sequence>
							</xs:restriction>
						</xs:complexContent>
					</xs:complexType>
					
					<xs:complexType name="Dre">
						<xs:complexContent>
							<xs:extension base="Dr"/>
						</xs:complexContent>
					</xs:complexType>
					
					<xs:complexType name="Der">
						<xs:complexContent>
							<xs:restriction base="De">
								<xs:sequence>
									<xs:element name="foo" type="empty"/>
								</xs:sequence>
							</xs:restriction>
						</xs:complexContent>
					</xs:complexType>
					
					<xs:complexType name="Dee">
						<xs:complexContent>
							<xs:extension base="De"/>
