<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://xstest-tns/schema11_D3_3_12_v01"
	xmlns:sv="http://xstest-tns/schema11_D3_3_12_v01">
	
<!-- 
	Test Case Type: Schema Valid test (Datatypes section 3.3.12) 
	Base Type     : gYear
	Description   : A day is a calendar (or "local time") day in each timezone, including the timezones
			    outside of +12:00 through -11:59 inclusive.
-->


<simpleType name="gMin">
	<restriction  base="gYear">
		<minInclusive value="2000+13:00"/>
	</restriction>
</simpleType>



<element name="root">
	<complexType>
		<sequence>
			<element name="elDate" type="sv:gMin" minOccurs="1" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</element>    

</schema>  
