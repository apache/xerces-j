<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <!--element of type xs:ENTITY -->
  <xs:element name="doc">
    <xs:complexType>
       <xs:sequence>
          <xs:element ref="para" maxOccurs="unbounded"/>
       </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="para">
    <xs:complexType>
        <xs:sequence>
            <xs:element name="e" type="xs:ENTITY" default="entity1"/>
        </xs:sequence>
    </xs:complexType>
  </xs:element>
  
 
</xs:schema>
