<?xml version="1.0" encoding="UTF-8"?>

<!--
Cannot use xs:anyAtomicType as base type of user-defined type
-->

<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  elementFormDefault="qualified">

  <xs:element name="e">
    <xs:simpleType>
        <xs:restriction base="xs:anyAtomicType"/>
    </xs:simpleType>
  </xs:element>

  
</xs:schema>
