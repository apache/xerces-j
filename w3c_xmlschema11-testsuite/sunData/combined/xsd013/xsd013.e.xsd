<!--
	HIGHLIGHT OF THIS TEST
	======================
	- reference to undeclared element
-->
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:foo="foo"
		targetNamespace="foo"
		elementFormDefault="qualified">
	
	<element name="root">
		<complexType>
			<sequence>
				<!-- don't be fooled! it's not foo:root -->
				<element ref="root" />
			</sequence>
		</complexType>
	</element>
</schema>
